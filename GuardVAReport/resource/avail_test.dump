		<field name="STIG_SEVERITY">CAT II</field>
		<field name="STIG_IACONTROLS">ECLP-1</field>
		<field name="STIG_SRG"></field>
	</row>
	<row>
		<field name="TEST_ID">2559</field>
		<field name="TEST_DESC">Oracle system privilege granted to users and roles</field>
		<field name="REPORT_ID">-1</field>
		<field name="CLASS_NAME">com.guardium.assessment.tests.QueryBasedTest</field>
		<field name="TEST_TYPE">4</field>
		<field name="AUDIT_CONFIG_TEMPLATE_ID">-1</field>
