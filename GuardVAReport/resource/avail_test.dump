		<field name="REPORT_ID">-1</field>
		<field name="CLASS_NAME">com.guardium.assessment.tests.parameter.cloudera.ParaHadoopUserGroupMapping</field>
		<field name="TEST_TYPE">2</field>
		<field name="AUDIT_CONFIG_TEMPLATE_ID">-1</field>
		<field name="DATASOURCE_TYPE_ID">25</field>
		<field name="THRESHOLD_REQUIRED">0</field>
